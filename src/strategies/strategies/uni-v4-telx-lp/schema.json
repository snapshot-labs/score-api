{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "TELx V4 LP Power",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Voting Symbol"
        },
        "registryAddress": {
          "type": "string",
          "title": "PositionRegistry Contract Address"
        },
        "telPrice": { "type": "number", "title": "Price of TEL in USD" },
        "tokens": {
          "type": "array",
          "title": "List of Token Decimals && Price in USD",
          "items": {
            "type": "object",
            "properties": {
              "address": { "type": "string", "title": "Token Address" },
              "decimals": { "type": "number", "title": "Token Decimals" },
              "price": { "type": "number", "title": "Token Price (USD)" }
            },
            "required": ["address", "decimals", "price"]
          }
        }
      },
      "required": ["registryAddress", "telPrice", "tokens"]
    }
  }
}
