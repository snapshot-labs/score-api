{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "DAO Name",
          "minLength": 1,
          "examples": ["e.g. apecoin, ens, gitcoin"]
        },
        "roles": {
          "type": "array",
          "title": "Discord Roles",
          "examples": ["e.g. Assembly, Trader, Moderator"],
          "items": {
            "type": "string",
            "title": "Discord role",
            "minLength": 1,
            "examples": ["e.g. Trader"]
          }
        }
      },
      "required": ["name", "roles"],
      "additionalProperties": false
    }
  }
}
